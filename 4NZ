TabMap:CreateSection("Loader")

TabMap:CreateInput({
    Name = "Map ID", 
    PlaceholderText = "71532913978389", 
    Callback = function(v) State.MapID = v end
})

TabMap:CreateButton({
    Name = "Load Custom Map",
    Callback = function()
        local s, r = pcall(function() return game:GetObjects("rbxassetid://"..State.MapID)[1] end)
        if s and r then
            r.Parent = game.Workspace
            if game.Players.LocalPlayer.Character then 
                r:MoveTo(game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,500,0)) 
            end
            Rayfield:Notify({Title = "Sucesso", Content = "Mapa carregado!", Duration = 5})
        else
            Rayfield:Notify({Title = "Erro", Content = "ID Inv√°lido ou Executor sem suporte", Duration = 5})
        end
    end,
})

