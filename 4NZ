-- [[ SE√á√ÉO DE FOV PERSONALIZADA ]] --
NewSection("FIELD OF VIEW (FOV)")

local Camera = workspace.CurrentCamera
local TargetFOV = 120

local function SetFOV(value)
    TargetFOV = value
    Camera.FieldOfView = value
    print("FOV alterado para: " .. value)
end

-- Bot√µes de FOV
NewButton("FOV: 110 (Padr√£o Competitivo)", function() SetFOV(110) end)
NewButton("FOV: 120 (Recomendado)", function() SetFOV(120) end)
NewButton("FOV: 220 (Extremo)", function() SetFOV(220) end)
NewButton("FOV: 270 (Quebrado/Quake)", function() SetFOV(270) end)

-- Trava de FOV
NewToggle("Fixar FOV Selecionado", function(state)
    getgenv().FixFOV = state
    task.spawn(function()
        while getgenv().FixFOV do
            if Camera.FieldOfView ~= TargetFOV then
                Camera.FieldOfView = TargetFOV
            end
            task.wait(0.1)
        end
    end)
end)

